{
  "name": "opuscodium-certificate_checker",
  "version": "2.0.0",
  "author": "Opus Codium",
  "summary": "Installs, configures, and manages Certificate checker",
  "license": "Apache-2.0",
  "source": "https://github.com/opus-codium/puppet-certificate_checker",
  "project_page": "https://github.com/opus-codium/puppet-certificate_checker",
  "issues_url": "https://github.com/opus-codium/puppet-certificate_checker/pulls",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.13.0 < 9.0.0"
    }
